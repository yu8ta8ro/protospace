.jumbotron
  .container.text-center
    = image_tag("header_logo.png", alt: "PROTO TYPE - SHARE THE PROTOTYPE", height: "73px")

.filter-nav
  .container
    .row
      %ul.nav.nav-pills.col-lg-11
        %li.active{role: "presentation"}
          %a{href: "#"} Popular PROTO
        %li{role: "presentation"}
          = link_to "Newest PROTO", new_prototype_path
      = button_to "ViewTags", tags_path, class: "btn btn-default col-lg-1", method: :get
          = link_to "Newest PROTO",  prototype_newest_index_path

      %button{type: "button", class: "btn btn-default col-lg-1"} View Tags

.container.proto-list
  .row
    = render partial: "prototypes/prototype", collection: @prototypes
.text-center
  %ul.pagination
    = paginate(@prototypes)
