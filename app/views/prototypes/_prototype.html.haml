.col-sm-4.col-md-3.proto-content
  .thumbnail
    = link_to image_tag(prototype.prototype_images), prototype_path(prototype.id)
    .caption
      %h3
        #{prototype.title}
      .proto-meta
        .proto-user
          %a{href: "#"} by #{prototype.user.name}
        .proto-posted
          #{prototype.created_at}
      %ul.proto-tag-list.list-inline
        %li
          %a{href: "#", class: "btn btn-default"} iPad
        %li
          %a{href: "#", class: "btn btn-default"} wireframe}

        %li
          = link_to "Edit", edit_prototype_path(prototype), class: "btn btn-default"
        %li
          = link_to "Delete", prototype_path(prototype), class: "btn btn-default", method: :delete
