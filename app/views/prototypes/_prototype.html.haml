.col-sm-4.col-md-3.proto-content
  .thumbnail
    %a{href: "/prototypes/show"}
      = image_tag("sample.png")
      .caption
        %h3
          #{prototype.title}
        .proto-meta
          .proto-user
            / %a{href: "#"} by #{prototype.user.name}
          .proto-posted
            #{prototype.created_at}
        %ul.proto-tag-list.list-inline
          %li
            %a{href: "#", class: "btn btn-default"} iPad
          %li
            %a{href: "#", class: "btn btn-default"} wireframe}

          %li
            = link_to "Edit", edit_prototype_path(prototype), class: "btn btn-default"
          %li
            = link_to "Delete", prototype, class: "btn btn-default", :method => :delete
